                 interface="com.jusfoun.hookah.rpc.api.PayBankCardService"
                 connectTimeout="30000" requestTimeout="30000" basicReferer="HookahRPCClientBasicConfig"/>
  <motan:referer id="payAccountRecordService"
                 interface="com.jusfoun.hookah.rpc.api.PayAccountRecordService"
                 connectTimeout="30000" requestTimeout="30000" basicReferer="HookahRPCClientBasicConfig"/>
  <motan:referer id="showService"
                 interface="com.jusfoun.hookah.rpc.api.ShowService"
                 connectTimeout="30000" requestTimeout="30000" basicReferer="HookahRPCClientBasicConfig"/>
