<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:motan="http://api.weibo.com/schema/motan"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
       http://api.weibo.com/schema/motan http://api.weibo.com/schema/motan.xsd">


    <!-- 业务具体实现类 -->
    <bean id="payAccountRecordService" class="com.jusfoun.hookah.core."/>

    <!-- 具体rpc服务配置，声明实现的接口类。-->
    <motan:service interface="com.jusfoun.hookah.rpc.api.PayAccountRecordService"
                   ref="payAccountRecordService" export="hookah:5001" basicService="serviceBasicConfig">
    </motan:service>
</beans>