<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.jusfoun.hookah.core.dao.GoodsMapper" >
  <!-- 批量插入 -->
  <insert id="insertBatch" parameterType="java.util.List">
    <selectKey keyProperty="goodsId" order="BEFORE" resultType="java.lang.String" keyColumn="goods_id">
      select replace(uuid(),'-','')
    </selectKey>
    insert into goods
    (
    goods_id,goods_name
    )
    values
    <foreach collection="list" index="index" item="item" separator=",">
      (
      #{goodsId,jdbcType=VARCHAR},#{item.goodsName,jdbcType=VARCHAR}
      )
    </foreach>
  </insert>
</mapper>